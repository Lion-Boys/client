{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "http://$SERVER_HOST/$1"
        },
        {
            "source": "/((?!api|assets|static|public|favicon\\.ico|robots\\.txt|manifest\\.webmanifest|service-worker\\.js|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}
